{
  "expo": {
    "name": "custom-slider",
    "slug": "custom-slider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/all.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hussain386.customslider"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/all.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hussain386.customslider"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/all.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
